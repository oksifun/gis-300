										</xs:annotation>
									</xs:element>
								</xs:choice>
								<xs:element name="ReportPeriodStatus">
									<xs:annotation>
										<xs:documentation>Статус отчетного периода</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Status">
												<xs:annotation>
													<xs:documentation>Статус</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="Posted">
															<xs:annotation>
																<xs:documentation>Размещен</xs:documentation>
															</xs:annotation>
														</xs:enumeration>
														<xs:enumeration value="Draft">
															<xs:annotation>
																<xs:documentation>Проект</xs:documentation>
															</xs:annotation>
														</xs:enumeration>
														<xs:enumeration value="Annul">
															<xs:annotation>
																<xs:documentation>Информация аннулирована</xs:documentation>
															</xs:annotation>
														</xs:enumeration>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="ReasonOfAnnulment" minOccurs="0">
