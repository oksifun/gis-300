											<xs:documentation>Месяц и год начала периода</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="EndPeriodYearMonth" type="xs:gYearMonth">
										<xs:annotation>
											<xs:documentation>Месяц и год окончания периода</xs:documentation>
										</xs:annotation>
									</xs:element>
