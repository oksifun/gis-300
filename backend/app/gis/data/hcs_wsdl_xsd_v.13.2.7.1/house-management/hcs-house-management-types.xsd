	<xs:simpleType name="MeetingTypeType">
		<xs:annotation>
			<xs:documentation>Тип собрания</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Owners"/>
			<xs:enumeration value="Homeowners"/>
			<xs:enumeration value="Cooperative"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MessageStatusType">
		<xs:annotation>
			<xs:documentation>Статус сообщения о проведении общего собрания</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Posted"/>
			<xs:enumeration value="Goes"/>
			<xs:enumeration value="Finished"/>
			<xs:enumeration value="MeetingCancelled"/>
			<xs:enumeration value="Cancelled"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="VotingSystemType">
		<xs:annotation>
			<xs:documentation>Система, в которой проводится собрание</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
