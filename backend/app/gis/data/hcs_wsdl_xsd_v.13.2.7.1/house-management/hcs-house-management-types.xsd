					<xs:documentation>Идентификатор помещения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BlockGUID" type="base:GUIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор блока жилого дома</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RoomGUID" type="base:GUIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор комнаты</xs:documentation>
				</xs:annotation>
