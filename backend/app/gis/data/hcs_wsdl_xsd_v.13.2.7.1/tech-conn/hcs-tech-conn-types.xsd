									<xs:element name="ConnectedResource" type="tns:EsnsiItemType" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Подключаемый ресурс. Справочник "Вид ресурсов или услуг, доступный для подключения" в ЕСНСИ</xs:documentation>
